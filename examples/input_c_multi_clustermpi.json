{
    "agent":{
        "type": "PPO",
        "policy": "MlpPolicy",
        "tensorboard_log": "tensorboard"
    },
    "environment": {
        "spline":{
            "spline_definition": {
                "space_dimensions": [
                    {
                        "name": "phi",
                        "number_of_points": 3,
                        "degree": 2,
                        "knot_vector": [
                            0.0,
                            0.0,
                            0.0,
                            1.0,
                            1.0,
                            1.0
                        ]
                    },
                    {
                        "name": "zeta",
                        "number_of_points": 3,
                        "degree": 2,
                        "knot_vector": [
                            0.0,
                            0.0,
                            0.0,
                            1.0,
                            1.0,
                            1.0
                        ]
                    }
                ],
                "spline_dimension": 2
            }
        },
        "mesh": {
            "path": "../../seminararbeit/sa072021-rl-shape-opti-framework/04-Data/ViscousFlowTJunction/mesh/coarse",
            "dimensions": 2,
            "export_path": "../../seminararbeit/sa072021-rl-shape-opti-framework/04-Data/ViscousFlowTJunction/mesh/deformed/_.xns"
        },
        "solver": {
            "multi_processor": {
                "max_core_count": 8,
                "location": "cluster"
            },
            "execution_command": "../../seminararbeit/sa072021-rl-shape-opti-framework/05-Code/bin/xnsmpi",
            "command_options": [
                "<",
                "xns_multi.in"
            ],
            "working_directory": "../../04-Data/ViscousFlowTJunction/",
            "reward_output": {
                "working_directory": "../../04-Data/ViscousFlowTJunction/",
                "reward_script": "../../04-Data/examples/get_reward_and_solver_observations.py"
            }
        },
        "multi_processing": {
            "number_of_cores": 4
        },
        "discrete_actions": true,
        "additional_observations": 2
    },
    "number_of_timesteps": 100000,
    "number_of_episodes": 1500,
    "save_location": "training_{}/",
    "verbosity": {
        "environment": "DEBUG",
        "parser": "DEBUG"
    },
    "validation": {
        "validation_freq": 3000,
        "validation_values": [
            1.95,
            0.24,
            0.12,
            1.1,
            0.5
        ],
        "save_best_agent": true,
        "validate_on_training_end": true,
        "mesh_base_path_extension": "mesh/_.xns",
        "max_timesteps_per_episode": 50,
        "end_episode_on_spline_not_changed": false
    }
}
